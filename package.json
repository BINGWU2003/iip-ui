{
  "name": "iip-ui-vue3",
  "version": "1.0.6",
  "description": "基于 Element Plus 的 Vue 3 组件库",
  "main": "dist/iip-ui-vue3.umd.js",
  "module": "dist/iip-ui-vue3.es.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "packages",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "dev:example": "vite serve examples",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{vue,js,ts,json,scss}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue3",
    "element-plus",
    "component-library",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "element-plus": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "element-plus": "^2.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "sass": "^1.0.0",
    "stylelint": "^14.0.0",
    "typescript": "^4.0.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.0.0",
    "vitepress": "^1.0.0-rc.10",
    "vitest": "^0.30.0",
    "vue": "^3.0.0",
    "vue-tsc": "^1.0.0"
  }
}
